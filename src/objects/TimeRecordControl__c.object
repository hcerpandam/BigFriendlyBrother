        <externalId>false</externalId>
        <formula>IF(
((HOUR(FirstCheckInsertion__c) - HOUR(FirstCheckTime__c))+(MINUTE(FirstCheckInsertion__c)-MINUTE(FirstCheckTime__c))/60)&lt;=0.1, 
0,
(HOUR(FirstCheckInsertion__c) - HOUR(FirstCheckTime__c))+(MINUTE(FirstCheckInsertion__c)-MINUTE(FirstCheckTime__c))/60)+
IF(
((HOUR(SecondCheckInsertion__c ) - HOUR(SecondCheckTime__c))+(MINUTE(SecondCheckInsertion__c )-MINUTE(SecondCheckTime__c))/60)&lt;=0.1, 
0,
(HOUR(SecondCheckInsertion__c ) - HOUR(SecondCheckTime__c))+(MINUTE(SecondCheckInsertion__c )-MINUTE(SecondCheckTime__c))/60)+
IF(
((HOUR(ThirdCheckInsertion__c ) - HOUR(ThirdCheckTime__c ))+(MINUTE(ThirdCheckInsertion__c )-MINUTE(ThirdCheckTime__c ))/60)&lt;=0.1, 
0,
(HOUR(ThirdCheckInsertion__c ) - HOUR(ThirdCheckTime__c ))+(MINUTE(ThirdCheckInsertion__c )-MINUTE(ThirdCheckTime__c ))/60)</formula>

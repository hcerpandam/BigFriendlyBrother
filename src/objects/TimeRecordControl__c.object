    <fields>
        <fullName>BreakRange__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
(((HOUR(FirstBreakEnd__c ) - HOUR(FirstBreakStart__c)) + ((MINUTE(FirstBreakEnd__c ) - MINUTE(FirstBreakStart__c ))/60))+
((HOUR(SecondBreakEnd__c) - HOUR(SecondBreakStart__c)) + ((MINUTE(FirstBreakEnd__c ) - MINUTE(FirstBreakStart__c ))/60))+
((HOUR(ThirdBreakEnd__c) - HOUR(ThirdBreakStart__c)) + ((MINUTE(ThirdBreakEnd__c) - MINUTE(ThirdBreakStart__c))/60)))&lt;=0.25
, 
0,
(((HOUR(FirstBreakEnd__c ) - HOUR(FirstBreakStart__c)) + ((MINUTE(FirstBreakEnd__c ) - MINUTE(FirstBreakStart__c ))/60))+
((HOUR(SecondBreakEnd__c) - HOUR(SecondBreakStart__c)) + ((MINUTE(FirstBreakEnd__c ) - MINUTE(FirstBreakStart__c ))/60))+
((HOUR(ThirdBreakEnd__c) - HOUR(ThirdBreakStart__c)) + ((MINUTE(ThirdBreakEnd__c) - MINUTE(ThirdBreakStart__c))/60)))-0.25)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duración descansos</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>

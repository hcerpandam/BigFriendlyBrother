        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>TotalRecordedByCompany__c</fullName>
        <externalId>false</externalId>
        <label>Total horas registradas por compañia</label>
        <summarizedField>TimeRecordControl__c.RecordedByCompany__c</summarizedField>
        <summaryForeignKey>TimeRecordControl__c.TimeRecordControlId__c</summaryForeignKey>

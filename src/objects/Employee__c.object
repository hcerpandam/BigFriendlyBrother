    <fields>
        <fullName>ByContractMonthlyWorkingHours__c</fullName>
        <description>Hours per month excluding weekends</description>
        <externalId>false</externalId>
        <formula>((( FLOOR ( ( DATE ( YEAR (DATE (YEAR(TODAY()),MONTH(TODAY()),28)+5), 
MONTH(DATE (YEAR(TODAY()),MONTH(TODAY()),28)+5), 1 ) - 1 - DATE (1900,01,07) ) / 7 ) *5 ) 
+ MIN ( 5, MOD ( DATE ( YEAR (DATE (YEAR(TODAY()),MONTH(TODAY()),28)+5), 
MONTH(DATE (YEAR(TODAY()),MONTH(TODAY()),28)+5), 1 ) - 1 - DATE (1900,01,07), 7 ) ) 
) - 
(( FLOOR ( ( DATE(YEAR(TODAY()),MONTH(TODAY()),1) - DATE (1900,01,07) ) / 7 ) *5 ) 
+ MIN ( 5, MOD ( DATE(YEAR(TODAY()),MONTH(TODAY()),1) - DATE (1900,01,07), 7 ) ) 
)) * 8</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Horas mensuales por contrato</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>

    <validationRules>
        <fullName>CompanyIdFormat</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS( CompanyId__c , &quot;techconsultant.com&quot;)  = False</errorConditionFormula>
        <errorDisplayField>CompanyId__c</errorDisplayField>
        <errorMessage>El Id de empresa no es correcto</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LinkedUserNotChanged</fullName>
        <active>true</active>
        <description>Comprobación de que sólo se ha asignado un usuario vinculado</description>
        <errorConditionFormula>AND( !ISBLANK(FullName__c) , ISNEW() = False  )</errorConditionFormula>
        <errorMessage>El usuario vinculado no puede cambiarse</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SameMobileAsUser</fullName>
        <active>true</active>
        <errorConditionFormula>MobilePhone__c  &lt;&gt; LinkedUser__r.MobilePhone</errorConditionFormula>
        <errorDisplayField>MobileNumber__c</errorDisplayField>
        <errorMessage>El teléfono del empleado debe ser igual al del usuario al que está vinculado</errorMessage>
    </validationRules>
